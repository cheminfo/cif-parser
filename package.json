{
  "name": "cif-to-json",
  "version": "1.0.0",
  "description": "Parsing xray CIF (Crystallographic Information File)",
  "main": "lib/index.js",
  "module": "src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "build": "cheminfo-build --entry src/index.js --root CIF",
    "eslint": "eslint src",
    "eslint-fix": "npm run eslint -- --fix",
    "prepublishOnly": "rollup -c",
    "test": "npm run test-coverage && npm run eslint",
    "test-coverage": "jest --coverage",
    "test-only": "jest"
  },
  "prettier": {
    "arrowParens": "always",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cheminfo-js/cif-to-json.git"
  },
  "keywords": [],
  "author": "Luc Patiny <luc@patiny.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheminfo-js/cif-to-json/issues"
  },
  "homepage": "https://github.com/cheminfo-js/cif-to-json#readme",
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.14.0",
    "@types/jest": "^26.0.23",
    "cheminfo-build": "^1.1.10",
    "cheminfo-publish": "^1.0.20",
    "codecov": "^3.8.2",
    "eslint": "^7.26.0",
    "eslint-config-cheminfo": "^5.2.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "prettier": "^2.3.0",
    "rollup": "^2.47.0"
  }
}
